<?xml version="1.0" encoding="UTF-8"?>
<xsp:page language="java"
  xmlns:xsp="http://apache.org/xsp"
  xmlns:xsp-request="http://apache.org/xsp/request/2.0"
  xmlns:soap="http://apache.org/xsp/soap/3.0"
  xmlns:xscript="http://apache.org/xsp/xscript/1.0"
  xmlns:log="http://apache.org/xsp/log/2.0"
  xmlns:session="http://apache.org/cocoon/session/1.0"
  xmlns:cinclude="http://apache.org/cocoon/include/1.0">
 <content>
 	<soap:call>
		<soap:url><xsp-request:get-parameter name="publishURL"/></soap:url>
 		<get_authToken cred="juddi" generic="2.0" userID="juddi" xmlns="urn:uddi-org:api_v2"/>
	</soap:call>
	<session:getxml context="request" path="parametervalues"/>
	<session:getxml context="mycontext" path="/"/>
	<pipeline><xsp-request:get-parameter name="pipeline"/></pipeline>
 </content>
</xsp:page>
