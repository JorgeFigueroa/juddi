<map:sitemap xmlns:map="http://apache.org/cocoon/sitemap/1.0">

<map:pipelines>
<!-- get authentification token -->
	<map:pipeline>
		<map:match pattern="get_auth_token">
			<map:generate type="serverpages" src="get_auth_token.xsp"/>
			<map:transform type="session"/>
			<map:transform src="get_auth_token.xsl"/>
			<map:serialize type="xml"/>
		</map:match>
	</map:pipeline>
<!-- empty path -->
	<map:pipeline>
		<map:match pattern="">
			<map:redirect-to session="true" uri="index"/>
		</map:match>
	</map:pipeline>
<!-- START -->
	<map:pipeline>
		<map:match pattern="index">
			<map:generate src="index.html"/>
			<map:serialize/>
		</map:match>
	</map:pipeline>

<!-- START UDDI -->
	<map:pipeline>
		<map:match pattern="start_uddi">
			<map:aggregate element="page">
			    <map:part src="start_uddi.xml" strip-root="false"/>
			    <map:part src="pf_header.xml" element="header" strip-root="false"/>
			    <map:part src="pf_nav.xml" element="nav" strip-root="false"/>
  			</map:aggregate>
			<map:transform type="session"/>
			<map:transform src="start_uddi.xsl"/>
			<map:serialize type="html"/>
		</map:match>
	</map:pipeline>

