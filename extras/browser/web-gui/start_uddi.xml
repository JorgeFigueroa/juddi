<content xmlns:session="http://apache.org/cocoon/session/1.0">
	<session:createcontext name="mycontext"/>
	<session:setxml context="mycontext" path="/">
		<session>
			<uddi>
				<inquiryURL><session:getxml context="request" path="/parameter/inquiryURL"/></inquiryURL>
				<publishURL><session:getxml context="request" path="/parameter/publishURL"/></publishURL>
				<name><session:getxml context="request" path="/parameter/name"/></name>
			</uddi>
		</session>
	</session:setxml>

	<session:getxml context="mycontext" path="/"/>

</content>
